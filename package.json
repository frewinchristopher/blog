{
  "name": "chrisfrew.in-blog",
  "private": true,
  "description": "A starter for a blog powered by Gatsby and Markdown",
  "version": "0.1.0",
  "author": "Chris Frewin <frewin.christopher@gmail.com>",
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "dependencies": {
    "disqus-react": "^1.0.8",
    "express-static-gzip": "^2.0.6",
    "gatsby": "^2.20.26",
    "gatsby-image": "^2.3.4",
    "gatsby-plugin-feed": "^2.4.3",
    "gatsby-plugin-google-analytics": "^2.2.4",
    "gatsby-plugin-manifest": "^2.3.5",
    "gatsby-plugin-offline": "^3.1.4",
    "gatsby-plugin-react-helmet": "^3.2.4",
    "gatsby-plugin-sharp": "^2.5.6",
    "gatsby-plugin-typography": "^2.4.3",
    "gatsby-remark-copy-linked-files": "^2.2.3",
    "gatsby-remark-emoji": "0.0.3",
    "gatsby-remark-images": "^3.2.4",
    "gatsby-remark-prismjs": "^3.4.4",
    "gatsby-remark-responsive-iframe": "^2.3.3",
    "gatsby-remark-smartypants": "^2.3.1",
    "gatsby-source-filesystem": "^2.2.4",
    "gatsby-transformer-remark": "^2.7.3",
    "gatsby-transformer-sharp": "^2.4.6",
    "prismjs": "^1.20.0",
    "react": "^16.13.1",
    "react-body-classname": "^1.3.1",
    "react-cookie": "^4.0.3",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-typography": "^0.16.19",
    "typeface-merriweather": "0.0.72",
    "typeface-montserrat": "0.0.75",
    "typography": "^0.16.19",
    "typography-theme-github": "^0.16.19",
    "typography-theme-wordpress-2016": "^0.16.19"
  },
  "devDependencies": {
    "minify": "^5.1.1",
    "prettier": "^1.19.1",
    "sass": "^1.26.5"
  },
  "homepage": "https://chrisfrew.in",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gatsbyjs/gatsby-starter-blog.git"
  },
  "scripts": {
    "build": "rm -rf public/* && sass --no-source-map src/styles-scss/global.scss src/styles/global.css && minify src/styles/global.css > src/styles/global.min.css && gatsby build && gzip -r public/",
    "dev": "git checkout develop && sass --no-source-map --watch src/styles-scss/global.scss src/styles/global.css && minify src/styles/global.css > src/styles/global.min.css & gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  }
}
